<section class="breadcrumb-option">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb__text">
                    <h4>Your Orders</h4>
                    <div class="breadcrumb__links">
                        <a href="./index.html">Home</a>
                        <a href="./shop.html">Shop</a>
                        <span>Your Orders</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>

<section class="shopping-cart spad">
    <div class="container">
        <% if(orders.length==0){%>
            <div class="d-flex justify-content-center">
              <img src="data:image/png;base64," alt="" />
              
               <!-- <h3>No Orders Available</h3> -->
            </div>
            <%} else{%>
        <div class="row">
            <div class="" style="width: 100%;">
                <div class="shopping__cart__table" >
                   
                    <table>
                        <thead>
                            <tr>
                                <th>Order Id</th>
                                <th>Date</th>
                                <th>Order Status</th>
                                <th>Option</th>
                            </tr>
                        </thead>
                        <tbody>
                            <% orders.forEach(function(orders) { %>
                            <tr>
                                <td class="product__cart__item">
                                    <a href="#" class="fw-bold" style="color: black; font-weight: bold;"> <%=orders.orders._id%>
                                </td>
                                <td class="quantity__item">
                                    <a href=""style="color: black; font-weight: bold;"><%=getDate(orders.orders.orderedDate)%></a>
                                </td>
                                <td class="cart__price"><%=orders.orders.orderStatus%></td>
                                <td class="cart__close"><a href="order-details?order=<%=orders.orders._id%>" class="btn btn-dark"> Order Details  </a></td>
                            </tr>
                            <% }) %> 
                            
                        </tbody>
                    </table>
                </div>
            </div>
            
        </div>
        <%}%>
    </div>
</section>