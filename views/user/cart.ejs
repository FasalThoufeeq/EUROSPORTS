    <script>
        function changeQuantity(cartId,proId,userId,count){

            let quantity=parseInt(document.getElementById(proId).textContent)
            count=parseInt(count)
            // console.log(quantity);
            $.ajax({
                url:"/change-quantity",
                method:"PUT",
                data:{user:userId, products:proId, cart:cartId, count:count, quantity:quantity},

                success:(response)=>{
                    if(response.removed==true){
                        alert("Do you want to remove this product from your cart ?")
                        location.reload()

                    }else{
                        let updatedQuantity=quantity+count
                        document.getElementById(proId).innerHTML=updatedQuantity
                        console.log(response.totalAmount[0].totalAmount);
                        document.getElementById('totalAmount').innerHTML=response.totalAmount[0].totalAmount
                        
                    }
                }
            })

        }




        function removeCart(cart,product){
            console.log(cart);
            console.log(product);
            $.ajax({
                url:"/removeCart",
                method:"DELETE",
                data:{cart,product},

                success:(response)=>{
                    if(response.removed==true){
                        alert("Do you want to remove this product from your cart")
                        location.reload()
                    }
                }

            })
        }

    </script>